{

  "manifest_version": 2,
  "name": "Pinned Tabs Fix NG",
  "version": "2.3.1",

  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/Exagone313/pinned-tabs-fix-ng",
  "default_locale": "en",

  "applications": {
    "gecko": {
      "id": "@pinned_tabs_fix_ng",
      "strict_min_version": "48.0"
    }
  },

  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },

  "background": {
    "scripts": ["background-script.js"]
  },

  "options_ui": {
    "page": "options/options.html"
  },

  "permissions": [
    "storage"
  ]

}
